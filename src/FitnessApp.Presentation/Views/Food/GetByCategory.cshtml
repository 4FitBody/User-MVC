@model IEnumerable<FitnessApp.Core.Foods.Food>

<div class="container">
    <div class="row">
        @foreach (var f in Model)
        {
            <div class="col-4">
                <p>@f.Title</p>
            </div>
        }
    </div>
</div>

<style>
    .col-4 {
        width: 33.33%;
        float: left;
        box-sizing: border-box;
        padding: 0 15px;
    }
</style>